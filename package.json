    "express": "^4.15.5",
    "fastlog": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "lodash": "^4.17.4",
    "mapbox-error": "^2.2.0",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "prettier": "^1.7.0",
    "sequelize": "^4.10.0"
  },
  "engines": {
